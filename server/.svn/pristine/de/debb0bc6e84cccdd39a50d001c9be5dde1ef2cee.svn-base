    public class VermellMilitiaDyeTub : DyeTub
    {
        [Constructable]
        public VermellMilitiaDyeTub()
        {
            Name = "Vermell Militia dye tub";
            Hue = DyedHue = MilitiaDyeHue.Vermell;
            Redyable = false;

        }

        public VermellMilitiaDyeTub(Serial serial)
            : base(serial)
        {
        }

        public override void Serialize(GenericWriter writer)
        {
            base.Serialize(writer);

            writer.Write((int)0); // version
        }

        public override void Deserialize(GenericReader reader)
        {
            base.Deserialize(reader);

            int version = reader.ReadInt();
        }
    }

